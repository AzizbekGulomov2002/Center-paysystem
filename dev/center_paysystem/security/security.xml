<odoo>
    <data>
        <!-- Access Control for Models -->
        <record id="model_center_paysystem_student_access" model="ir.model.access">
            <field name="name">student access</field>
            <field name="model_id" ref="model_center_paysystem_student"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <record id="model_center_paysystem_teacher_access" model="ir.model.access">
            <field name="name">teacher access</field>
            <field name="model_id" ref="model_center_paysystem_teacher"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
    </data>
</odoo>
